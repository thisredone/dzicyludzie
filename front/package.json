{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "yarn run build && firebase deploy"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "firebase": "^7.5.0",
    "firebaseui": "^4.3.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-coffeescript": "^0.0.3",
    "vue-cli-plugin-tailwind": "^1.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": "./tailwind.js",
      "./util/purgecss.js": {
        "whitelistPatternsChildren": [
          "firebaseui.*",
          "^bg.*"
        ],
        "content": [
          "src/**/*.vue"
        ]
      },
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
